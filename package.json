{
  "name": "extenv",
  "version": "1.0.0",
  "description": "A module for loading .env files with default environmental files using schema for validating.",
  "keywords": [
    "extenv",
    "env",
    ".env",
    "environment",
    "variables",
    "config",
    "default",
    "defaults",
    "settings"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "require": "./lib/index.js",
      "default": "./lib/index.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "npm run clean && tsc && rollup -c",
    "test": "jest",
    "clean": "rimraf ./lib",
    "prune": "npm run clean && rimraf ./node_modules"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dPaskhin/extenv.git"
  },
  "files": [
    "lib"
  ],
  "author": "Dmitrii Paskhin <d.pasxin@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dPaskhin/extenv/issues"
  },
  "homepage": "https://github.com/dPaskhin/extenv#readme",
  "readmeFilename": "README.md",
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@types/jest": "^29.5.3",
    "@types/node": "^20.4.4",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "jest": "^29.6.1",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.1",
    "rollup": "^3.26.3",
    "ts-jest": "^29.1.1"
  },
  "dependencies": {
    "dotenv": "^16.3.1",
    "doubter": "^2.1.0"
  },
  "engines": {
    "node": ">=12"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
